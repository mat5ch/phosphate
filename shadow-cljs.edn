{:deps true
 :builds
 {:extension {:target :node-library
              :js-options {:js-provider :shadow
                           :keep-native-requires true
                           :keep-as-require #{"vscode"}}
              :compiler-options {:infer-externs :auto}
              :output-dir "out/js"
              :output-to "out/phosphate.js"
              :exports {:activate phosphate.extension/activate}
              :devtools {:before-load-async phosphate.extension/before
                         :after-load phosphate.extension/after}}}}